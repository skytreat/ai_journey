{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "AzureTableStorage": "UseDevelopmentStorage=true"
  },
  "AzureStorage": {
    "TableName": "ipamtest",
    "UseDevelopmentStorage": true
  },
  "Authentication": {
    "DevAuth": {
      "Enabled": true,
      "DefaultUser": "testuser",
      "DefaultRoles": ["SystemAdmin"]
    }
  },
  "RateLimiting": {
    "Enabled": false,
    "MaxRequestsPerWindow": 1000,
    "WindowSizeInSeconds": 60
  },
  "OpenTelemetry": {
    "Enabled": false
  }
}
